    "chai": "^4.2.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^4.3.0",
    "mocha": "^6.2.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
