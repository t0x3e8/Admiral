{
  "name": "battleship.mvc.vue",
  "version": "0.0.2",
  "description": "Asp.net Core MVC and Vue",
  "main": "index.js",
  "scripts": {
    "code:restore": "dotnet restore BackEnd",
    "code:build": "dotnet build BackEnd",
    "code:run": "dotnet BackEnd/bin/Debug/netcoreapp3.1/code.dll",
    "dev": "concurrently \"run-s code:restore code:build code:run\" \"webpack --config=webpack.dev.js --watch\"",
    "build": "webpack --config=webpack.prod.js",
    "test:frontend": "mocha FrontEnd_Test/*/*.test.js --require @babel/register",
    "test:backend": "dotnet test BackEnd_Test --verbosity minimal --nologo --no-restore",
    "test": "run-s test:* -c"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/t0x3e8/Core-and-Vue-boilerplate.git"
